              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute type="mule:expressionString" use="optional" name="keystore">
            <xs:annotation>
              <xs:documentation>Path to the keystore file.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
